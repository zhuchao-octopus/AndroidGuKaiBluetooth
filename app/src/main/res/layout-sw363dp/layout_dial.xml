                <ImageButton
                    android:id="@+id/n4"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_4" />

                <ImageButton
                    android:id="@+id/n5"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:layout_marginLeft="6.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_5" />

                <ImageButton
                    android:id="@+id/n6"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:layout_marginLeft="6.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_6" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6.0px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n7"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_7" />

                <ImageButton
                    android:id="@+id/n8"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:layout_marginLeft="6.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_8" />

                <ImageButton
                    android:id="@+id/n9"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:layout_marginLeft="6.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_9" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="6.0px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/nx"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_s" />

                <ImageButton
                    android:id="@+id/n0"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:layout_marginLeft="6.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_0" />

                <ImageButton
                    android:id="@+id/nj"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:layout_marginLeft="6.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_h" />
            </LinearLayout>

