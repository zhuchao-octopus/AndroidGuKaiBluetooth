<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_dial"
    android:layout_width="fill_parent"
    android:layout_height="645.0px"
    android:paddingLeft="60px"
    android:paddingTop="40px"
    android:paddingRight="60px"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="127.0px"
        android:background="@drawable/dialnumberbg"
        android:gravity="center_vertical">

        <LinearLayout
            android:layout_width="720.0px"
            android:layout_height="fill_parent"
            android:orientation="vertical"
            android:paddingBottom="0px"
            android:paddingTop="6.0px">

            <TextView
                android:id="@+id/digit"
                android:layout_width="fill_parent"
                android:layout_height="78.0px"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:textSize="50px" />

            <TextView
                android:id="@+id/dial_status"
                android:layout_width="fill_parent"
                android:layout_height="36.0px"
                android:gravity="center"
                android:textColor="#ffe0e0e0"
                android:textSize="24.0px" />
        </LinearLayout>

        <ImageView
            android:id="@+id/delete"
            android:layout_width="216.0px"
            android:layout_height="fill_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="24.0px"
            android:scaleType="centerInside"
            android:background="@drawable/dial_back"
            android:onClick="onClick" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:paddingLeft="36.0px"
        android:paddingTop="6.0px">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n1"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:background="@drawable/rec_btn"
                    android:clickable="true"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_1" />

                <ImageButton
                    android:id="@+id/n2"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:layout_marginLeft="6.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_2" />

                <ImageButton
                    android:id="@+id/n3"
                    android:layout_width="216.0px"
                    android:layout_height="103.0px"
                    android:layout_marginLeft="6.0px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:scaleType="centerCrop"
                    android:src="@drawable/b_3" />
