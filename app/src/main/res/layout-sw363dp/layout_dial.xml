            <!--                     android:src="@drawable/call3_add" /> -->

            <!--                 <ImageButton -->
            <!--                     android:id="@+id/call3_swtich" -->
            <!--                     android:layout_width="180px" -->
            <!--                     android:layout_height="77px" -->
            <!--                     android:layout_marginLeft="15px" -->
            <!--                     android:background="@drawable/rec_btn" -->
            <!--                     android:onClick="onDClick" -->
            <!--                     android:src="@drawable/call3_switch" /> -->

            <!--                 <ImageButton -->
            <!--                     android:id="@+id/call3_merge" -->
            <!--                     android:layout_width="180px" -->
            <!--                     android:layout_height="77px" -->
            <!--                     android:layout_marginLeft="15px" -->
            <!--                     android:background="@drawable/rec_btn" -->
            <!--                     android:onClick="onDClick" -->
            <!--                     android:src="@drawable/call3_merge" /> -->
            <!--             </LinearLayout> -->
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="216.0px"
                android:layout_height="211.0px"
                android:layout_marginLeft="6.0px"
                android:layout_marginTop="0px"
                android:background="@drawable/key_nor"
                android:orientation="vertical"
                android:paddingBottom="4.0px"
                android:paddingLeft="5.0px"
                android:paddingRight="5.0px"
                android:paddingTop="4.0px">

                <ImageButton
                    android:id="@+id/dial_dialout"
                    android:layout_width="match_parent"
                    android:layout_height="103.0px"
                    android:background="@drawable/rec_btn_dial"
                    android:onClick="onDialclick"
                    android:scaleType="centerInside"
                    android:src="@drawable/ico_dailout" />

                <ImageButton
                    android:id="@+id/dial_handup"
                    android:layout_width="match_parent"
                    android:layout_height="103.0px"
                    android:background="@drawable/rec_btn_hang"
                    android:onClick="onDialclick"
                    android:src="@drawable/ico_handup" />
            </LinearLayout>

            <ImageButton
                android:id="@+id/dial_voiceswitch"
                android:layout_width="216.0px"
