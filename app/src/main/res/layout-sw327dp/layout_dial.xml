<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_dial"
    android:layout_width="844px"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="105px"
        android:background="#162632"
        android:orientation="vertical"
        android:paddingBottom="5px"
        android:paddingTop="5px">

        <TextView
            android:id="@+id/digit"
            android:layout_width="fill_parent"
            android:layout_height="60px"
            android:gravity="center"
            android:singleLine="true"
            android:textColor="@android:color/white"
            android:textSize="42px" />

        <TextView
            android:id="@+id/dial_status"
            android:layout_width="fill_parent"
            android:layout_height="35px"
            android:gravity="center"
            android:textColor="#ffe0e0e0"
            android:textSize="28px" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:background="#0c151d"
        android:orientation="horizontal"
        android:paddingLeft="60px"
        android:paddingTop="20px">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n1"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:background="@drawable/rec_btn"
                    android:clickable="true"
                    android:onClick="onDClick"
                    android:src="@drawable/b_1" />

                <ImageButton
                    android:id="@+id/n2"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_2" />

                <ImageButton
                    android:id="@+id/n3"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_3" />

                <ImageView
                    android:id="@+id/delete"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onClick"
                    android:src="@drawable/delete_nor" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="18.75px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n4"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_4" />

                <ImageButton
                    android:id="@+id/n5"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_5" />

                <ImageButton
                    android:id="@+id/n6"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_6" />

                <ImageButton
                    android:id="@+id/dial_dialout"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn_dial"
                    android:onClick="onDialclick"
                    android:src="@drawable/ico_dailout" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="18.75px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n7"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_7" />

                <ImageButton
                    android:id="@+id/n8"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_8" />

                <ImageButton
                    android:id="@+id/n9"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_9" />

                <ImageButton
                    android:id="@+id/dial_handup"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn_hang"
                    android:onClick="onDialclick"
                    android:src="@drawable/ico_handup" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="18.75px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/nx"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_s" />

                <ImageButton
                    android:id="@+id/n0"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_0" />

                <ImageButton
                    android:id="@+id/nj"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_h" />

                <ImageButton
                    android:id="@+id/dial_voiceswitch"
                    android:layout_width="168.75px"
                    android:layout_height="80.599976px"
                    android:layout_marginLeft="18.75px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDialclick"
                    android:src="@drawable/ico_btpswitch" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>