<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/tr_dial"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">

            <LinearLayout
                android:layout_width="560px"
                android:layout_height="78px"
                android:background="@drawable/button_click_back2"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0px"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/digit"
                        android:layout_width="256.0dip"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:singleLine="true"
                        android:textColor="@android:color/white"
                        android:textSize="22.0sp" />

                    <TextView
                        android:id="@+id/dial_status"
                        android:layout_width="110.0dip"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10.0dip"
                        android:gravity="center"
                        android:textColor="#ffe0e0e0"
                        android:textSize="18.0sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layout_3call"
                    android:layout_width="match_parent"
                    android:layout_height="0px"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/digit_3call"
                        android:layout_width="256.0dip"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:singleLine="true"
                        android:textColor="@android:color/white"
                        android:textSize="22.0sp" />

                    <TextView
                        android:id="@+id/dial_status_3call"
                        android:layout_width="110.0dip"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10.0dip"
                        android:gravity="center"
                        android:textColor="#ffe0e0e0"
                        android:textSize="18.0sp" />
                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/delete"
                style="@style/ButonStyleNum3Call"
                android:onClick="onClick"
                android:scaleType="center"
                android:src="@drawable/bt_num_del" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">

                <TextView
                    android:id="@+id/n1"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_1" />

                <TextView
                    android:id="@+id/n2"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_2" />

                <TextView
                    android:id="@+id/n3"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_3" />

                <TextView
                    android:id="@+id/nx"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_x0" />

                <ImageButton
                    android:id="@+id/call3_add"
                    style="@style/ButonStyleNum3Call"
                    android:onClick="onDialclick"
                    android:src="@drawable/call3_add" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">

                <TextView
                    android:id="@+id/n4"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_4" />

                <TextView
                    android:id="@+id/n5"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_5" />

                <TextView
                    android:id="@+id/n6"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_6" />

                <TextView
                    android:id="@+id/n0"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_0" />

                <ImageButton
                    android:id="@+id/call3_swtich"
                    style="@style/ButonStyleNum3Call"
                    android:onClick="onDialclick"
                    android:src="@drawable/call3_switch" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center">

                <TextView
                    android:id="@+id/n7"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_7" />

                <TextView
                    android:id="@+id/n8"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_8" />

                <TextView
                    android:id="@+id/n9"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_9" />

                <TextView
                    android:id="@+id/nj"
                    style="@style/ButonStyleNum3Call"
                    android:text="@string/digit_x1" />

                <ImageButton
                    android:id="@+id/call3_merge"
                    style="@style/ButonStyleNum3Call"
                    android:onClick="onDialclick"
                    android:src="@drawable/call3_merge" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>