        <item name="android:shadowDx">2</item>
        <item name="android:shadowDy">2</item>
    </style>

    <style name="shadowTextCommonStyleFonts" parent="shadowText">
        <item name="android:fontFamily">casual</item>
    </style>

    <style name="CommonStyleFonts">
        <item name="android:fontFamily">casual</item>
    </style>

    <style name="ButonStyleNum" parent="CommonStyleFonts">
        <item name="android:background">@drawable/button_click_back2</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:layout_width">175px</item>
        <item name="android:layout_height">78px</item>
        <item name="android:textSize">40.0px</item>
        <item name="android:onClick">onDClick</item>
        <item name="android:gravity">center</item>
        <item name="android:focusable">true</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ButonStyleNum3Call" parent="CommonStyleFonts">
        <item name="android:background">@drawable/button_click_back2</item>
