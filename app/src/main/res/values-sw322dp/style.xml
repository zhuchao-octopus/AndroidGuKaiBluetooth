<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ButtonStyle1">
        <item name="android:background">@drawable/button_click_back1</item>
        <item name="android:textColor">#A4CFF2</item>
        <item name="android:layout_height">90px</item>
        <item name="android:layout_width">200px</item>
        <item name="android:gravity">center</item>
        <item name="android:focusable">true</item>
        <item name="android:scaleType">center</item>
    </style>

    <style name="shadowText">
        <item name="android:shadowColor">#ff000000</item>
