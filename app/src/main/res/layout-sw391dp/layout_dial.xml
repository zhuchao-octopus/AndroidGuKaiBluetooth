<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_dial"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5.0dip">

        <ImageView
            android:layout_width="512.0dip"
            android:layout_height="65.0dip"
            android:layout_marginLeft="30.0dip"
            android:layout_marginTop="6.0dip" />

        <!-- <ImageView -->
        <!-- android:id="@id/dial_status" -->
        <!-- android:layout_width="46.0dip" -->
        <!-- android:layout_height="46.0dip" -->
        <!-- android:layout_marginLeft="30.0dip" -->
        <!-- android:layout_marginTop="18.0dip" -->
        <!-- android:background="@drawable/phonestatus" -->
        <!-- android:visibility="invisible" /> -->

        <TextView
            android:id="@+id/digit"
            android:layout_width="246.0dip"
            android:layout_height="51.0dip"
            android:layout_marginLeft="77.0dip"
            android:layout_marginTop="15.0dip"
            android:gravity="center"
            android:textColor="@android:color/white"
            android:textSize="22.0sp" />

        <TextView
            android:id="@+id/dial_status"
            android:layout_width="110.0dip"
            android:layout_height="51.0dip"
            android:layout_marginLeft="330.0dip"
            android:layout_marginTop="18.0dip"
            android:gravity="center"
            android:textColor="#ffe0e0e0"
            android:textSize="18.0sp" />

        <ImageView
            android:id="@+id/delete"
            android:layout_width="72.0dip"
            android:layout_height="51.0dip"
            android:layout_marginLeft="563.0dip"
            android:layout_marginTop="15.0dip"
            android:onClick="onClick"
            android:background="@drawable/dial_back" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30.0dip"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n1"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:background="@drawable/rec_btn"
                    android:clickable="true"
                    android:onClick="onDClick"
                    android:src="@drawable/b_1" />

                <ImageButton
                    android:id="@+id/n2"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_2" />

                <ImageButton
                    android:id="@+id/n3"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_3" />

                <ImageButton
                    android:id="@+id/nj"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_h" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5.0dip"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n4"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_4" />

                <ImageButton
                    android:id="@+id/n5"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_5" />

                <ImageButton
                    android:id="@+id/n6"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_6" />

                <ImageButton
                    android:id="@+id/nx"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_s" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5.0dip"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n7"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_7" />

                <ImageButton
                    android:id="@+id/n8"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_8" />

                <ImageButton
                    android:id="@+id/n9"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_9" />

                <ImageButton
                    android:id="@+id/n0"
                    android:layout_width="123.0dip"
                    android:layout_height="65.0dip"
                    android:layout_marginLeft="5.0dip"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_0" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="110.0dip"
            android:layout_height="fill_parent"
            android:layout_marginLeft="5.0dip"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/dial_dialout"
                android:layout_width="fill_parent"
                android:layout_height="97.0px"
                android:background="@drawable/rec_btn"
                android:onClick="onDialclick"
                android:src="@drawable/ico_dailout" />

            <ImageButton
                android:id="@+id/dial_handup"
                android:layout_width="fill_parent"
                android:layout_height="97.0px"
                android:layout_marginTop="9.0px"
                android:background="@drawable/rec_btn"
                android:onClick="onDialclick"
                android:src="@drawable/ico_handup" />

            <ImageButton
                android:id="@+id/dial_voiceswitch"
                android:layout_width="fill_parent"
                android:layout_height="97.0px"
                android:layout_marginTop="9.0px"
                android:background="@drawable/rec_btn"
                android:onClick="onDialclick"
                android:src="@drawable/ico_btpswitch" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>