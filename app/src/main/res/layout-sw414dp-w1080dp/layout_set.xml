<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_set"
    android:layout_width="fill_parent"
    android:layout_height="720px"
    android:layout_gravity="top"
    android:gravity="center"
    android:paddingRight="80px"
    android:paddingLeft="80px"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4.0dip"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="241.0dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="102.0dip"
                android:singleLine="true"
                android:text="@string/device_name"
                android:textColor="#ffc0c0c0"
                android:textSize="36px" />

            <TextView
                android:id="@+id/btname"
                android:layout_width="318.0dip"
                android:layout_height="66.0dip"
                android:layout_marginLeft="33.0dip"
                android:background="@drawable/setup_textbg"
                android:ellipsize="end"
                android:gravity="center"
                android:inputType="textFilter"
                android:onClick="onSetClick"
                android:singleLine="true"
                android:tag="ak_paramter=96"
                android:textColor="#ffc0c0c0"
                android:textSize="36px" />
        </LinearLayout>

        <ImageView
            android:layout_width="1346.0px"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="5.0dip"
            android:layout_marginRight="5.0dip"
            android:layout_marginTop="1.0dip"
            android:background="@drawable/history_listsplice" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="1.0dip"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="241.0dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="102.0dip"
                android:singleLine="true"
                android:text="@string/pin"
                android:textColor="#ffc0c0c0"
                android:textSize="36px" />

            <TextView
                android:id="@+id/btpin"
                android:layout_width="318.0dip"
                android:layout_height="66.0dip"
                android:layout_marginLeft="43.0dip"
                android:background="@drawable/setup_textbg"
                android:gravity="center"
                android:maxLength="4"
                android:numeric="integer"
                android:onClick="onSetClick"
                android:singleLine="true"
                android:tag="ak_paramter=96"
                android:textColor="#ffc0c0c0"
                android:textSize="36px" />
        </LinearLayout>

        <ImageView
            android:layout_width="1346.0px"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="5.0dip"
            android:layout_marginRight="5.0dip"
            android:layout_marginTop="1.0dip"
            android:background="@drawable/history_listsplice" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="66.0dip"
            android:layout_marginTop="1.0dip"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="241.0dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="102.0dip"
                android:singleLine="true"
                android:text="@string/auto_answer"
                android:textColor="#ffc0c0c0"
                android:textSize="36px" />

            <ImageView
                android:id="@+id/autoanswercheck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="134.0dip"
                android:background="@drawable/onoff"
                android:clickable="true"
                android:onClick="onSetClick"
                android:tag="ak_paramter=96" />
        </LinearLayout>

        <ImageView
            android:layout_width="1346.0px"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="5.0dip"
            android:layout_marginRight="5.0dip"
            android:layout_marginTop="1.0dip"
            android:background="@drawable/history_listsplice" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="66.0dip"
            android:layout_marginTop="1.0dip"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="241.0dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="102.0dip"
                android:singleLine="true"
                android:text="@string/auto_connect"
                android:textColor="#ffc0c0c0"
                android:textSize="36px" />

            <ImageView
                android:id="@+id/autoconnectcheck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="134.0dip"
                android:background="@drawable/onoff"
                android:clickable="true"
                android:onClick="onSetClick"
                android:tag="ak_paramter=96" />
        </LinearLayout>

        <ImageView
            android:layout_width="1346.0px"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="5.0dip"
            android:layout_marginRight="5.0dip"
            android:layout_marginTop="1.0dip"
            android:background="@drawable/history_listsplice" />
    </LinearLayout>

    <ListView
        android:id="@+id/t_list"
        style="@style/CommonListViewStyle1"
        android:layout_width="726px"
        android:layout_height="fill_parent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0px"
        android:background="#ff000000"
        android:cacheColorHint="@android:color/transparent"
        android:dividerHeight="2px"
        android:fadingEdge="none"
        android:focusable="true"
        android:paddingBottom="4px"
        android:paddingTop="4px"
        android:scrollbars="none" />

</LinearLayout>