<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_set"
    android:layout_width="631.0px"
    android:layout_height="344.0px"
    android:background="@drawable/bt_t_list_bg_n1"
    android:gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="625.0px"
            android:layout_height="65.0px"
            android:background="@drawable/bt_t_bg_n1"
            android:clickable="true"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="126.0px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56.0px"
                android:gravity="center_vertical|left"
                android:singleLine="true"
                android:text="@string/device_name"
                android:textColor="#ffebebeb"
                android:textSize="22px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/btname"
                android:layout_width="170.0px"
                android:layout_height="48.0px"
                android:layout_marginLeft="136.0px"
                android:background="@drawable/setup_textbg"
                android:ellipsize="end"
                android:gravity="center"
                android:inputType="textFilter"
                android:onClick="onSetClick"
                android:singleLine="true"
                android:tag="ak_paramter=96"
                android:textColor="#ffebebeb"
                android:textSize="22px"
                android:textStyle="bold" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4.0px"
            android:layout_marginRight="4.0px"
            android:background="@drawable/bt_t_list_divider_n1" />

        <LinearLayout
            android:layout_width="625.0px"
            android:layout_height="52.0px"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="126.0px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56.0px"
                android:singleLine="true"
                android:text="@string/pin"
                android:gravity="center_vertical|left"
                android:textColor="#ffebebeb"
                android:textSize="22px"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/btpin"
                android:layout_width="170.0px"
                android:layout_height="48.0px"
                android:layout_marginLeft="136.0px"
                android:background="@drawable/setup_textbg"
                android:gravity="center"
                android:maxLength="4"
                android:numeric="integer"
                android:onClick="onSetClick"
                android:singleLine="true"
                android:tag="ak_paramter=96"
                android:textColor="#ffebebeb"
                android:textSize="22px"
                android:textStyle="bold" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4.0px"
            android:layout_marginRight="4.0px"
            android:background="@drawable/bt_t_list_divider_n1" />

        <LinearLayout
            android:layout_width="625.0px"
            android:layout_height="65.0px"
            android:layout_marginTop="1.0px"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="156.0px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56.0px"
                android:singleLine="true"
                android:text="@string/auto_connect"
                android:textColor="#ffebebeb"
                android:textSize="22px"
                android:gravity="center_vertical|left"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/autoconnectcheck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="123.0px"
                android:background="@drawable/onoff"
                android:clickable="true"
                android:onClick="onSetClick"
                android:tag="ak_paramter=96" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4.0px"
            android:layout_marginRight="4.0px"
            android:background="@drawable/bt_t_list_divider_n1" />

        <LinearLayout
            android:layout_width="625.0px"
            android:layout_height="65.0px"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="156.0px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="56.0px"
                android:singleLine="true"
                android:text="@string/auto_answer"
                android:textColor="#ffebebeb"
                android:gravity="center_vertical|left"
                android:textSize="22px"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/autoanswercheck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="123.0px"
                android:background="@drawable/onoff"
                android:clickable="true"
                android:onClick="onSetClick"
                android:tag="ak_paramter=96" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4.0px"
            android:layout_marginRight="4.0px"
            android:background="@drawable/bt_t_list_divider_n1" />
    </LinearLayout>

    <ListView
        android:id="@+id/t_list"
        style="@style/CommonListViewStyle1"
        android:layout_width="566.0px"
        android:layout_height="fill_parent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0px"
        android:background="#ff000000"
        android:cacheColorHint="@android:color/transparent"
        android:dividerHeight="2px"
        android:fadingEdge="none"
        android:focusable="true"
        android:paddingBottom="4px"
        android:paddingTop="4px"
        android:scrollbars="none" />

</LinearLayout>