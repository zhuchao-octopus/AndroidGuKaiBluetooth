<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/lefticonbackground"
        android:layout_width="182.0px"
        android:layout_height="687px"
        android:layout_gravity="center|left"
        android:background="@drawable/bt_l_bg_n1">

        <ImageView
            android:id="@+id/menu"
            android:layout_width="102.0px"
            android:layout_height="102.0px"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="37.0px"
            android:layout_marginTop="86.0px"
            android:background="@drawable/bt_l_tb_bg_n1"
            android:clickable="true"
            android:onClick="onClick"
            android:scaleType="center"
            android:src="@drawable/bt_menu_n1" />

        <ImageView
            android:id="@+id/home"
            android:layout_width="126.0px"
            android:layout_height="126.0px"
            android:layout_centerVertical="true"
            android:layout_marginLeft="19.0px"
            android:background="@drawable/bt_l_c_bg_n1"
            android:clickable="true"
            android:onClick="onClick"
            android:scaleType="center"
            android:src="@drawable/bt_home_n1" />

        <ImageView
            android:id="@+id/back"
            android:layout_width="102.0px"
            android:layout_height="102.0px"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="86.0px"
            android:layout_marginLeft="37.0px"
            android:background="@drawable/bt_l_tb_bg_n1"
            android:clickable="true"
            android:onClick="onClick"
            android:scaleType="center"
            android:src="@drawable/bt_back_n1" />
    </RelativeLayout>

    <!-- <View -->
    <!-- android:layout_width="fill_parent" -->
    <!-- android:layout_height="1.0px" -->
    <!-- android:background="#626262" -->
    <!-- android:gravity="center_vertical" /> -->

    <LinearLayout
        android:layout_width="809.0px"
        android:layout_height="441.0px"
        android:layout_marginTop="30px"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="20.0px"
        android:layout_marginRight="150.0px"
        android:gravity="top"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/main_page_no_dial"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/bt_tr_pb_bg_n1"
            android:gravity="center">

            <LinearLayout
                android:layout_width="620.0px"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:gravity="top|left"
                android:orientation="horizontal">

                <com.zhuchao.android.bt.view.MarqueeTextView
                    android:id="@+id/disconnect_info"
                    style="@style/shadowTextCommonStyleFonts"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="#ffffffff"
                    android:textSize="24px"
                    android:visibility="gone" />
            </LinearLayout>

            <!-- <ListView -->
            <!-- android:id="@+id/t_list" -->
            <!-- style="@style/CommonListViewStyle1" -->
            <!-- android:layout_width="726px" -->
            <!-- android:layout_height="wrap_content" -->
            <!-- android:layout_centerHorizontal="true" -->
            <!-- android:layout_marginTop="0px" -->
            <!-- android:background="#00000000" -->
            <!-- android:cacheColorHint="@android:color/transparent" -->
            <!-- android:dividerHeight="2px" -->
            <!-- android:fadingEdge="none" -->
            <!-- android:focusable="true" -->
            <!-- android:paddingBottom="4px" -->
            <!-- android:paddingTop="4px" -->
            <!-- android:scrollbars="none" -->
            <!-- android:visibility="gone" /> -->

            <LinearLayout
                android:id="@+id/t_normal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="0px"
                android:gravity="center"
                android:orientation="horizontal"
                android:visibility="visible">

                <RelativeLayout
                    android:layout_width="620.0px"
                    android:layout_height="441.0px"
                    android:layout_marginLeft="0.0px"
                    android:layout_marginRight="1.0px"
                    android:gravity="center">

                    <RelativeLayout
                        android:id="@+id/tr_list_bg"
                        android:layout_width="match_parent"
                        android:layout_height="392px"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:gravity="center_horizontal"
                        android:visibility="visible">

                        <EditText
                            android:id="@+id/tr_edit"
                            android:layout_width="680px"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="-8px"
                            android:layout_marginLeft="100px"
                            android:layout_marginRight="0px"
                            android:layout_marginTop="0px"
                            android:gravity="center"
                            android:imeOptions="actionDone"
                            android:inputType="text"
                            android:singleLine="true"
                            android:visibility="gone" />

                        <ListView
                            android:id="@+id/tr_list"
                            style="@style/CommonListViewStyle1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignWithParentIfMissing="true"
                            android:layout_below="@+id/tr_edit"
                            android:layout_centerInParent="true"
                            android:cacheColorHint="@android:color/transparent"
                            android:dividerHeight="2px"
                            android:fadingEdge="none"
                            android:focusable="true"
                            android:paddingBottom="2px"
                            android:paddingLeft="0px"
                            android:paddingRight="2px"
                            android:paddingTop="2px"
                            android:scrollbars="none" />
                    </RelativeLayout>
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginRight="5px"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/tl_0"
                        style="@style/ButonStyleLeft" />

                    <ImageView
                        android:id="@+id/tl_1"
                        style="@style/ButonStyleLeft" />

                    <ImageView
                        android:id="@+id/tl_2"
                        style="@style/ButonStyleLeft" />

                    <ImageView
                        android:id="@+id/tl_3"
                        style="@style/ButonStyleLeft" />
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <include
        layout="@layout/layout_a2dp"
        android:visibility="gone" />

    <include
        android:layout_width="809.0px"
        android:layout_height="441.0px"
        android:layout_marginTop="30px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="150.0px"
        layout="@layout/layout_pair"
        android:visibility="gone" />

    <include
        android:layout_width="809.0px"
        android:layout_height="441.0px"
        android:layout_marginTop="30px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="150.0px"
        layout="@layout/layout_dial"
        android:visibility="gone" />

    <include
        android:layout_width="809.0px"
        android:layout_height="441.0px"
        android:layout_marginTop="30px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="150.0px"
        layout="@layout/layout_set"
        android:visibility="gone" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/lefticonbackground"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:layout_marginBottom="20px"
        android:paddingRight="10.0px">

        <LinearLayout
            android:layout_width="870.0px"
            android:layout_height="104.0px"
            android:layout_gravity="bottom|center|right"
            android:layout_marginBottom="4.0px"
            android:background="@drawable/bt_b_bg_n1"
            android:gravity="center">

            <ImageView
                android:id="@+id/dial"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@drawable/bt_b_l_bg_n1"
                android:clickable="true"
                android:onClick="onBClick"
                android:scaleType="center"
                android:src="@drawable/bt_page_dial" />

            <ImageView
                android:id="@+id/calllog"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@drawable/bt_b_c_bg_n1"
                android:clickable="true"
                android:onClick="onBClick"
                android:scaleType="center"
                android:src="@drawable/bt_icon_his" />

            <ImageView
                android:id="@+id/phonebook"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@drawable/bt_b_c_bg_n1"
                android:clickable="true"
                android:onClick="onBClick"
                android:scaleType="center"
                android:src="@drawable/bt_icon_pb" />

            <ImageView
                android:id="@+id/a2dp"
                android:layout_width="0.0dip"
                android:layout_height="75.0px"
                android:layout_weight="1.0"
                android:background="@drawable/bt_b_c_bg_n1"
                android:clickable="true"
                android:onClick="onBClick"
                android:scaleType="center"
                android:src="@drawable/bt_page_a2dp"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/pair"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@drawable/bt_b_c_bg_n1"
                android:clickable="true"
                android:onClick="onBClick"
                android:scaleType="center"
                android:src="@drawable/bt_icon_device" />

            <ImageView
                android:id="@+id/setting"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:background="@drawable/bt_b_r_bg_n1"
                android:clickable="true"
                android:onClick="onBClick"
                android:scaleType="center"
                android:src="@drawable/bt_icon_settings" />
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/downloading"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#cc000000"
        android:clickable="true"
        android:gravity="center"
        android:textColor="#ffffffff"
        android:textSize="36px"
        android:visibility="gone" />

</RelativeLayout>