<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_set"
    android:layout_width="809.0px"
    android:layout_height="441.0px"
    android:background="@drawable/bt_t_list_bg_n1"
    android:gravity="center"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="801.0px"
            android:layout_height="83.0px"
            android:background="@drawable/bt_t_bg_n1"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="161.0px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="72.0px"
                android:gravity="center_vertical|left"
                android:singleLine="true"
                android:text="@string/device_name"
                android:textColor="#ffebebeb"
                android:textSize="16.0sp"
                android:textStyle="bold" />

            <RelativeLayout
                android:layout_width="218.0px"
                android:layout_height="58.0px"
                android:layout_marginLeft="163.0px">

                <TextView
                    android:layout_width="218.0px"
                    android:layout_height="58.0px"
                    android:background="@drawable/setup_textbg"
                    android:clickable="false"
                    android:focusable="false" />

                <TextView
                    android:id="@+id/btname"
                    android:layout_width="218.0px"
                    android:layout_height="58.0px"
                    android:background="@drawable/com_focus_select"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:inputType="textFilter"
                    android:onClick="onSetClick"
                    android:singleLine="true"
                    android:tag="ak_paramter=96"
                    android:textColor="#ffebebeb"
                    android:textSize="16.0sp"
                    android:textStyle="bold" />
            </RelativeLayout>
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0px"
            android:layout_marginRight="5.0px"
            android:background="@drawable/bt_t_list_divider_n1" />

        <LinearLayout
            android:layout_width="801.0px"
            android:layout_height="83.0px"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="162.0px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="72.0px"
                android:gravity="center_vertical|left"
                android:singleLine="true"
                android:text="@string/pin"
                android:textColor="#ffebebeb"
                android:textSize="16.0sp"
                android:textStyle="bold" />

            <RelativeLayout
                android:layout_width="218.0px"
                android:layout_height="58.0px"
                android:layout_marginLeft="163.0px">

                <TextView
                    android:layout_width="218.0px"
                    android:layout_height="58.0px"
                    android:background="@drawable/setup_textbg"
                    android:clickable="false"
                    android:focusable="false" />

                <TextView
                    android:id="@+id/btpin"
                    android:layout_width="218.0px"
                    android:layout_height="58.0px"
                    android:background="@drawable/com_focus_select"
                    android:gravity="center"
                    android:maxLength="4"
                    android:numeric="integer"
                    android:onClick="onSetClick"
                    android:singleLine="true"
                    android:tag="ak_paramter=96"
                    android:textColor="#ffebebeb"
                    android:textSize="16.0sp"
                    android:textStyle="bold" />
            </RelativeLayout>
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0px"
            android:layout_marginRight="5.0px"
            android:background="@drawable/bt_t_list_divider_n1" />

        <LinearLayout
            android:layout_width="801.0px"
            android:layout_height="83.0px"
            android:layout_marginTop="1.0px"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="162.0px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="72.0px"
                android:gravity="center_vertical|left"
                android:singleLine="true"
                android:text="@string/auto_connect"
                android:textColor="#ffebebeb"
                android:textSize="16.0sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/autoconnectcheck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="194.0px"
                android:background="@drawable/com_focus_select"
                android:clickable="true"
                android:onClick="onSetClick"
                android:src="@drawable/onoff"
                android:tag="ak_paramter=96" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0px"
            android:layout_marginRight="5.0px"
            android:background="@drawable/bt_t_list_divider_n1" />

        <LinearLayout
            android:layout_width="801.0px"
            android:layout_height="83.0px"
            android:gravity="center|left"
            android:orientation="horizontal">

            <TextView
                android:layout_width="162.0px"
                android:layout_height="wrap_content"
                android:layout_marginLeft="72.0px"
                android:gravity="center_vertical|left"
                android:singleLine="true"
                android:text="@string/auto_answer"
                android:textColor="#ffebebeb"
                android:textSize="16.0sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/autoanswercheck"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="194.0px"
                android:background="@drawable/com_focus_select"
                android:clickable="true"
                android:onClick="onSetClick"
                android:src="@drawable/onoff"
                android:tag="ak_paramter=96" />
        </LinearLayout>

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0px"
            android:layout_marginRight="5.0px"
            android:background="@drawable/bt_t_list_divider_n1" />
    </LinearLayout>

    <ListView
        android:id="@+id/t_list"
        style="@style/CommonListViewStyle1"
        android:layout_width="726px"
        android:layout_height="fill_parent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0px"
        android:background="#ff000000"
        android:cacheColorHint="@android:color/transparent"
        android:dividerHeight="2px"
        android:fadingEdge="none"
        android:focusable="true"
        android:paddingBottom="4px"
        android:paddingTop="4px"
        android:scrollbars="none" />

</LinearLayout>