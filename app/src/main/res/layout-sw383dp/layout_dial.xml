<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_dial"
    android:layout_width="fill_parent"
    android:layout_height="460px"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingTop="80px">

    <RelativeLayout
        android:layout_width="916px"
        android:layout_height="87px"
        android:paddingLeft="10px"
        android:gravity="center_vertical">

        <LinearLayout
            android:layout_width="710px"
            android:layout_height="110px"
            android:background="@drawable/dialnumberbg"
            android:orientation="horizontal"
            android:paddingBottom="0px"
            android:paddingTop="5px">

            <LinearLayout
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/digit"
                    android:layout_width="match_parent"
                    android:layout_height="46px"
                    android:gravity="center"
                    android:textColor="@android:color/white"
                    android:textSize="34px" />

                <TextView
                    android:id="@+id/dial_status"
                    android:layout_width="fill_parent"
                    android:layout_height="32px"
                    android:gravity="center"
                    android:textColor="#ffe0e0e0"
                    android:textSize="22px" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_3call"
                android:layout_width="0px"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    android:id="@+id/digit_3call"
                    android:layout_width="match_parent"
                    android:layout_height="50px"
                    android:gravity="center"
                    android:textColor="@android:color/white"
                    android:textSize="34px" />

                <TextView
                    android:id="@+id/dial_status_3call"
                    android:layout_width="fill_parent"
                    android:layout_height="32px"
                    android:gravity="center"
                    android:textColor="#ffe0e0e0"
                    android:textSize="22px" />
            </LinearLayout>
        </LinearLayout>

        <ImageButton
            android:id="@+id/delete"
            android:layout_width="162px"
            android:layout_height="77px"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20px"
            android:layout_marginRight="15px"
            android:background="@drawable/rec_btn"
            android:onClick="onClick"
            android:src="@drawable/delete_nor" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="916px"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:paddingLeft="10px"
        android:paddingTop="15px">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageButton
                            android:id="@+id/n1"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:background="@drawable/rec_btn"
                            android:clickable="true"
                            android:onClick="onDClick"
                            android:src="@drawable/b_1" />

                        <ImageButton
                            android:id="@+id/n2"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:layout_marginLeft="20px"
                            android:background="@drawable/rec_btn"
                            android:onClick="onDClick"
                            android:src="@drawable/b_2" />

                        <ImageButton
                            android:id="@+id/n3"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:layout_marginLeft="20px"
                            android:background="@drawable/rec_btn"
                            android:onClick="onDClick"
                            android:src="@drawable/b_3" />

                        <ImageButton
                            android:id="@+id/nx"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:layout_marginLeft="20px"
                            android:background="@drawable/rec_btn"
                            android:onClick="onDClick"
                            android:src="@drawable/b_s" />

                        <!--                         <ImageButton -->
                        <!--                             android:id="@+id/call3_add" -->
                        <!--                             android:layout_width="162px" -->
                        <!--                             android:layout_height="77px" -->
                        <!--                             android:layout_marginLeft="20px" -->
                        <!--                             android:background="@drawable/rec_btn" -->
                        <!--                             android:onClick="onDialclick" -->
                        <!--                             android:src="@drawable/call3_add" /> -->

                        <ImageButton
                            android:id="@+id/dial_dialout"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:layout_marginLeft="20px"
                            android:background="@drawable/rec_btn2"
                            android:onClick="onDialclick"
                            android:src="@drawable/ico_dailout" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15px"
                        android:orientation="horizontal">

                        <ImageButton
                            android:id="@+id/n4"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:background="@drawable/rec_btn"
                            android:onClick="onDClick"
                            android:src="@drawable/b_4" />

                        <ImageButton
                            android:id="@+id/n5"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:layout_marginLeft="20px"
                            android:background="@drawable/rec_btn"
                            android:onClick="onDClick"
                            android:src="@drawable/b_5" />

                        <ImageButton
                            android:id="@+id/n6"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:layout_marginLeft="20px"
                            android:background="@drawable/rec_btn"
                            android:onClick="onDClick"
                            android:src="@drawable/b_6" />

                        <ImageButton
                            android:id="@+id/n0"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:layout_marginLeft="20px"
                            android:background="@drawable/rec_btn"
                            android:onClick="onDClick"
                            android:src="@drawable/b_0" />

                        <!--                         <ImageButton -->
                        <!--                             android:id="@+id/call3_swtich" -->
                        <!--                             android:layout_width="162px" -->
                        <!--                             android:layout_height="77px" -->
                        <!--                             android:layout_marginLeft="20px" -->
                        <!--                             android:background="@drawable/rec_btn" -->
                        <!--                             android:onClick="onDialclick" -->
                        <!--                             android:src="@drawable/call3_switch" /> -->

                        <ImageButton
                            android:id="@+id/dial_handup"
                            android:layout_width="162px"
                            android:layout_height="77px"
                            android:layout_marginLeft="20px"
                            android:background="@drawable/rec_btn"
                            android:onClick="onDialclick"
                            android:src="@drawable/ico_handup" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n7"
                    android:layout_width="162px"
                    android:layout_height="77px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_7" />

                <ImageButton
                    android:id="@+id/n8"
                    android:layout_width="162px"
                    android:layout_height="77px"
                    android:layout_marginLeft="20px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_8" />

                <ImageButton
                    android:id="@+id/n9"
                    android:layout_width="162px"
                    android:layout_height="77px"
                    android:layout_marginLeft="20px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_9" />

                <ImageButton
                    android:id="@+id/nj"
                    android:layout_width="162px"
                    android:layout_height="77px"
                    android:layout_marginLeft="20px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_h" />

                <!--                 <ImageButton -->
                <!--                     android:id="@+id/call3_merge" -->
                <!--                     android:layout_width="162px" -->
                <!--                     android:layout_height="77px" -->
                <!--                     android:layout_marginLeft="20px" -->
                <!--                     android:background="@drawable/rec_btn" -->
                <!--                     android:onClick="onDialclick" -->
                <!--                     android:src="@drawable/call3_merge" /> -->

                <ImageButton
                    android:id="@+id/dial_voiceswitch"
                    android:layout_width="162px"
                    android:layout_height="77px"
                    android:layout_marginLeft="20px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDialclick"
                    android:src="@drawable/ico_btpswitch" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>