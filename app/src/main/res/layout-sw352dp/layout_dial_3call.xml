<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_dial"
    android:layout_width="931.0px"
    android:layout_height="344.0px"
    android:background="@drawable/bt_tr_d_bg_n1_3call"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="56.0px"
        android:layout_marginBottom="1.0px">

        <!-- <ImageView
            android:layout_width="580.0px"
            android:layout_height="65.0px"
            android:layout_marginLeft="30.0px"
            android:layout_marginTop="6.0px" /> -->

        <!-- <ImageView -->
        <!-- android:id="@id/dial_status" -->
        <!-- android:layout_width="46.0dip" -->
        <!-- android:layout_height="46.0dip" -->
        <!-- android:layout_marginLeft="30.0dip" -->
        <!-- android:layout_marginTop="18.0dip" -->
        <!-- android:background="@drawable/phonestatus" -->
        <!-- android:visibility="invisible" /> -->

        <TextView
            android:id="@+id/digit"
            android:layout_width="466.0px"
            android:layout_height="40.0px"
            android:layout_marginLeft="43.0px"
            android:layout_marginTop="12.0px"
            android:gravity="center"
            android:textColor="#ffebebeb"
            android:textSize="30.0px"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dial_status"
            android:layout_width="90.0px"
            android:layout_height="40.0px"
            android:layout_marginLeft="520.0px"
            android:layout_marginTop="14.0px"
            android:gravity="right"
            android:textColor="#ffebebeb"
            android:textSize="26.0px"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/delete"
            android:layout_width="56.0px"
            android:layout_height="40.0px"
            android:layout_marginLeft="800.0px"
            android:layout_marginTop="12.0px"
            android:src="@drawable/dial_back"
            android:onClick="onClick" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="720.0px"
                android:layout_height="95.0px"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n1"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:clickable="true"
                    android:onClick="onDClick"
                    android:src="@drawable/b_1" />

                <ImageButton
                    android:id="@+id/n2"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_2" />

                <ImageButton
                    android:id="@+id/n3"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_3" />

                <ImageButton
                    android:id="@+id/nj"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_h" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="720.0px"
                android:layout_height="95.0px"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n4"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_4" />

                <ImageButton
                    android:id="@+id/n5"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_5" />

                <ImageButton
                    android:id="@+id/n6"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_6" />

                <ImageButton
                    android:id="@+id/nx"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_s" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="720.0px"
                android:layout_height="95.0px"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n7"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_7" />

                <ImageButton
                    android:id="@+id/n8"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_8" />

                <ImageButton
                    android:id="@+id/n9"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_9" />

                <ImageButton
                    android:id="@+id/n0"
                    android:layout_width="0.0px"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:scaleType="center"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_0" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="140.0px"
            android:layout_height="286.0px"
            android:layout_marginLeft="30.0px"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/dial_dialout"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="@drawable/bt_tr_r_d_bg_n1"
                android:onClick="onDialclick"
                android:src="@drawable/ico_dailout" />

            <ImageButton
                android:id="@+id/dial_handup"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="@drawable/bt_tr_r_d_bg_n1"
                android:onClick="onDialclick"
                android:src="@drawable/ico_handup" />

            <ImageButton
                android:id="@+id/dial_voiceswitch"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="@drawable/bt_tr_r_d_bg_n1"
                android:onClick="onDialclick"
                android:paddingLeft="39.0px"
                android:paddingRight="39.0px"
                android:paddingTop="17.0px"
                android:paddingBottom="17.0px"
                android:scaleType="fitCenter"
                android:src="@drawable/ico_btpswitch" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>