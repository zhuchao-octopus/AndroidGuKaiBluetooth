<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tr_dial"
    android:layout_width="813px"
    android:layout_height="540px"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="110px"
        android:background="@drawable/dialnumberbg"
        android:gravity="center_vertical">

        <LinearLayout
            android:layout_width="600px"
            android:layout_height="110px"
            android:orientation="vertical"
            android:paddingBottom="5px"
            android:paddingTop="5px">

            <TextView
                android:id="@+id/digit"
                android:layout_width="fill_parent"
                android:layout_height="60px"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:textSize="50px" />

            <TextView
                android:id="@+id/dial_status"
                android:layout_width="fill_parent"
                android:layout_height="35px"
                android:gravity="center"
                android:textColor="#ffe0e0e0"
                android:textSize="24px" />
        </LinearLayout>

        <ImageView
            android:id="@+id/delete"
            android:layout_width="84.0dip"
            android:layout_height="62.0dip"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="50px"
            android:background="@drawable/dial_back"
            android:onClick="onClick" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:paddingLeft="30px"
        android:paddingTop="5px">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n1"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:background="@drawable/rec_btn"
                    android:clickable="true"
                    android:onClick="onDClick"
                    android:src="@drawable/b_1" />

                <ImageButton
                    android:id="@+id/n2"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:layout_marginLeft="15px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_2" />

                <ImageButton
                    android:id="@+id/n3"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:layout_marginLeft="15px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n4"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_4" />

                <ImageButton
                    android:id="@+id/n5"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:layout_marginLeft="15px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_5" />

                <ImageButton
                    android:id="@+id/n6"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:layout_marginLeft="15px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_6" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/n7"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_7" />

                <ImageButton
                    android:id="@+id/n8"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:layout_marginLeft="15px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_8" />

                <ImageButton
                    android:id="@+id/n9"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:layout_marginLeft="15px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_9" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15px"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/nx"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_s" />

                <ImageButton
                    android:id="@+id/n0"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:layout_marginLeft="15px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_0" />

                <ImageButton
                    android:id="@+id/nj"
                    android:layout_width="180px"
                    android:layout_height="90px"
                    android:layout_marginLeft="15px"
                    android:background="@drawable/rec_btn"
                    android:onClick="onDClick"
                    android:src="@drawable/b_h" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="180px"
                android:layout_height="190px"
                android:layout_marginLeft="15px"
                android:layout_marginTop="5px"
                android:paddingLeft="4px"
                android:paddingTop="3px"
                android:paddingBottom="3px"
                android:paddingRight="4px"
                android:background="@drawable/key_nor"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/dial_dialout"
                    android:layout_width="172px"
                    android:layout_height="92px"
                    android:background="@drawable/rec_btn_dial"
                    android:onClick="onDialclick"
                    android:src="@drawable/ico_dailout" />

                <ImageButton
                    android:id="@+id/dial_handup"
                    android:layout_width="172px"
                    android:layout_height="92px"
                    android:background="@drawable/rec_btn_hang"
                    android:onClick="onDialclick"
                    android:src="@drawable/ico_handup" />

            </LinearLayout>

            <ImageButton
                android:id="@+id/dial_voiceswitch"
                android:layout_width="180px"
                android:layout_height="195px"
                android:layout_marginTop="15px"
                android:layout_marginLeft="15px"
                android:background="@drawable/rec_btn"
                android:onClick="onDialclick"
                android:src="@drawable/ico_btpswitch" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>